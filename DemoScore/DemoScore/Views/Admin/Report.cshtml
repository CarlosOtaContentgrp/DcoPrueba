@using DemoScore.Models;
@using DemoScore.Models.ViewModel;
@model AdminReports
@if (TempData["Menssages"] != null)
{
    @section Scripts {
        <script type="text/JavaScript">
            $(document).ready(function () {
                $("#Menssages").modal();
            });
        </script>
    }}



<div id="Menssages" class="modal fade" role="dialog">
    <div class="modal-dialog1">
        <div class="modal-content">
            <div class="modal-body">
                <p>
                    @TempData["Menssages"]
                </p>
            </div>
            <br />
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>
<br />
<br /><br />
<h1 class="title">GESTIÓN REPORTES</h1>


<div class="row fecha">
    <div class="btn-group btn-breadcrumb">
        <a href="@Url.Action("Index", "Home")" class="btn btn-info"><i class="glyphicon glyphicon-home"></i></a>
    </div>
</div>



<div class="jumbotron3">

    <div class="col-md-3">
        <a href='@Url.Action("ReporteCategoria", "Admin", new {id =@Model.company_Id})'>
            <img src='@Url.Content("~/Content/Imagenes/usuarios_registrados.png")' style="width:90%; height:80%;" />
        </a>
        <h4>@Html.ActionLink("Reporte de categorias", "ReporteCategoria", "Admin", new { ReturnUrl = ViewBag.ReturnUrl, id =@Model.company_Id}, new { @class = "", role = "form", @style = "background-color:transparent;color:#000;" })</h4>
    </div>
    <div class="col-md-3">
        <a href='@Url.Action("ReporteGeneral", "Admin", new {id =@Model.company_Id})'>
            <img src='@Url.Content("~/Content/Imagenes/progreso_curso.png")' style="width:90%; height:80%;" />
        </a>
        <h4>@Html.ActionLink("Progreso General", "ReporteGeneral", "Admin", new { ReturnUrl = ViewBag.ReturnUrl, id = @Model.company_Id }, new { @class = "", role = "form", @style = "background-color:transparent;color:#000;" })</h4>
    </div>
    <div class="col-md-3">
        <a href='@Url.Action("ReporteResultadosusuario", "Admin", new {id =@Model.company_Id})'>
            <img src='@Url.Content("~/Content/Imagenes/usuarios_registrados.png")' style="width:90%; height:80%;" />
        </a>
        <h4>@Html.ActionLink("Progreso usuario", "ReporteResultadosusuario", "Admin", new { ReturnUrl = ViewBag.ReturnUrl, id = @Model.company_Id }, new { @class = "", role = "form", @style = "background-color:transparent;color:#000;" })</h4>
    </div>
    <div class="col-md-3">
        <a href='@Url.Action("Reporteresultadoscategorias", "Admin", new {id =@Model.company_Id})'>
            <img src='@Url.Content("~/Content/Imagenes/progreso_curso.png")' style="width:90%; height:80%;" />
        </a>
        <h4>@Html.ActionLink("Progreso Categorias", "Reporteresultadoscategorias", "Admin", new { ReturnUrl = ViewBag.ReturnUrl, id = @Model.company_Id }, new { @class = "", role = "form", @style = "background-color:transparent;color:#000;" })</h4>
    </div>
</div>

