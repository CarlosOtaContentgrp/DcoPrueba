
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title DCO</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0,maximum-scale=1.0,minimum-scade=1.0">
    <title>@ViewBag.Title - DCO</title>
    @using Microsoft.AspNet.Identity
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/demo.css")
    @Styles.Render("~/Content/style1.css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>


    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="col-md-12 text-center logo_inicialHeader">
                    <a href='@Url.Action("Index", "Home")'>
                        <div style='width:150px;height:50px;'>
                            <p><img class="img-responsive" src="~/Content/Imagenes/LOGO DCO-15.png" border="0" width="1280" height="1000" /></p>  
                            </div>
                           
</a>
                </div>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="container containerA">
        @RenderBody()
        <hr />
    </div>
    <div class="container containerA navbar-inverse col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin-top:130px;">

        <div class="logo-footer"> </div>

        <div class="info-footer">
            <br>
            Desarrollado por:
            <a href="http://contentgrp.com/" target="_blank" style = "background-color:transparent;color:#000;">Content Group</a>
        </div><br><br>

        <div class="info-footer">
            <a href="http://contentgrp.com/" target="_blank" style = "background-color:transparent;color:#000;">Politicas de privacidad</a>
        </div><br>

        <div class="info-footer">
            <a href="http://contentgrp.com/" target="_blank" style = "background-color:transparent;color:#000;">Terminos y condiciones</a>
        </div>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
